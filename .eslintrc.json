{
	"extends": "airbnb",
	"rules": {
		"no-tabs": 0,
		"no-console": 0,
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"arrow-parens": ["error", "always"],
		"react/jsx-indent": ["error", "tab"],
		"react/jsx-indent-props": ["error", "tab"],
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
	}
}
